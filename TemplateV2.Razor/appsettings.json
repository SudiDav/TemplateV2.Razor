{
  "ConnectionStrings": {
    "DefaultConnection": "Server=.;Database=templatev2;Trusted_Connection=True;",
    "Timeout": 5000
  },

  "Cache": {
    "IsEnabled": true,
    "ExpiryTimeMinutes": 5
  },

  "Serilog": {
    "WriteTo": [
      "Console",
      "DiagnosticTrace"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    }
  },

  "Email": {
    "IsEnabled": false,
    "SendGrid": {
      "APIKey": "insert-api-key-here"
    },
    "SystemEmailAddress": "system@example.com",
    "SystemEmailName": "TemplateV2.Razor"
  },

  "ApplicationInsights": {
    "InstrumentationKey": "insert-instrumentation-key-here"
  }
}
