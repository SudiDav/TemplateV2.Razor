{
  "ConnectionStrings": {
    "DefaultConnection": "Server=.;Database=templatev2;Trusted_Connection=True;",
    "Timeout": 5000
  },

  "Cache": {
    "UseMemoryCache": true,
    "ExpiryTimeMinutes": 5
  },

  "Serilog": {
    "WriteTo": [
      "Console",
      "DiagnosticTrace",
      {
        "Name": "Seq",
        "Args": { "serverUrl": "http://localhost:5341" }
      }
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    }
  },

  "Email": {
    "IsEnabled": false,
    "SendGrid": {
      "APIKey": "insert-api-key-here"
    },
    "SystemEmailAddress": "system@example.com",
    "SystemEmailName": "TemplateV2.Razor"
  },
  "ApplicationInsights": {
    "InstrumentationKey": "a232f148-cc1b-4fd8-b39e-b52e895a607a"
  }
}
